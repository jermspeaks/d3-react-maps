{
  "name": "d3-react-maps",
  "version": "0.1.0",
  "description": "d3 with react for maps",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha \"test\" --compilers js:babel-register --recursive",
    "test:watch": "NODE_ENV=test mocha \"test\" --compilers js:babel-register --recursive --watch",
    "lint": "eslint src --cache",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm test -- --reporter dot",
    "postversion": "git push --follow-tags",
    "start": "NODE_ENV=development webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jermspeaks/d3-react-maps.git"
  },
  "keywords": [
    "d3",
    "react",
    "maps"
  ],
  "author": "Jeremy Wong <jermspeaks@gmail.com> (http://craftbyzen.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jermspeaks/d3-react-maps/issues"
  },
  "homepage": "https://github.com/jermspeaks/d3-react-maps#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^2.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^3.16.1",
    "expect": "^1.14.0",
    "html-webpack-plugin": "^2.9.0",
    "husky": "^0.11.1",
    "mocha": "^2.4.5",
    "path": "^0.12.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
